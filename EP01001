
#include <bits/stdc++.h>
int main (){
	int t;
	scanf("%d",&t);
	while(t--){
		char s[1000];
		scanf("%s",s);
		int len = strlen(s);
		for(int i=0;i<len;i++){
		if(s[i] <= 'Z' && s[i] >= 'A' ){
			s[i] += 32;
		}
		else if(s[i] >= 'a' && s[i] <= 'z'){
			s[i] -= 32;
		}
		for(int i=0;i<len;i++){
		printf("%c",s[i]);
		}
		printf("\n");
		}
		}
		return 0;
}
